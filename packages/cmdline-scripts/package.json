{
  "name": "cmdline-scripts",
  "version": "0.0.1",
  "description": "Build scripts for Node.js command-line applications",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "./node_modules/.bin/cross-env BABEL_ENV=production ./node_modules/.bin/babel-node src/Build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "cmdline-scripts": "./bin/cmdline-scripts.js"
  },
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          "stage-2"
        ]
      },
      "production": {
        "presets": [
          [
            "env"
          ],
          "stage-2"
        ],
        "plugins": [
          "transform-runtime"
      ]
    },
      "binaries": {
        "presets": [
          [
            "env"
          ],
          "stage-2"
        ],
        "plugins": [
          [
            "./build/babel-plugins/prepender",
            {
              "text": [
                "Â© 2017-present Harald Rudell <harald.rudell@gmail.com> (http://www.haraldrudell.com)",
                "This source code is licensed under the ISC-style license found in the LICENSE file in the root directory of this source tree."
              ]
            }
          ],
          "transform-runtime"
        ]
      },
      "plugins": {
        "presets": [
          [
            "env"
          ],
          "stage-2"
        ]
      }
    }
  },
  "keywords": [
    "app"
  ],
  "private": true,
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "fs-extra": "^4.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5"
  }
}
