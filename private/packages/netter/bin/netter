    exit(1);
  }

  static getDefaults(o) {
    let { m, debug, exit } = o || false;
    if (typeof exit !== 'function') exit = process.exit;
    debug = Boolean(debug);
    if (!m || typeof m !== 'string') m = 'OnRejected';
    return { m, debug, exit };
  }
