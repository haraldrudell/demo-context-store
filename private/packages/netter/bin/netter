  for (let basename of [`${name}-${hostname}.yaml`, `${name}.yaml`]) paths.push(basename, path.join(os.homedir(), 'apps', basename), path.join('..', basename), path.join('/etc', basename));

  for (let aPath of paths) {
    if (await lib.pathExists(aPath)) return aPath;
  }
  if (!noneIsOk) throw new Error(`Parameter files not found: ${paths.join(', ')}`);
}

