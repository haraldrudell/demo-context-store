{
  "name": "rollupapp",
  "version": "0.0.1",
  "description": "Demonstrate app assembly using Rollup",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "helloworld": "../../node_modules/.bin/rollup --config 0-helloworld/rollup.config.js",
    "es2017": "../../node_modules/.bin/rollup --config 1-es2017/rollup.config.js",
    "runclass": "../../node_modules/.bin/rollup --config 2-runclass/rollup.config.js",
    "nobabel": "../../node_modules/.bin/rollup --config 3-no-rollup-babel/rollup.config.js",
    "cleanerr": "../../node_modules/.bin/rollup --config 4-clean/rollup.config.js",
    "babelenv": "../../node_modules/.bin/rollup --config 5-babel-env/rollup.config.js",
    "helpers": "../../node_modules/.bin/rollup --config 6-babelhelpers/rollup.config.js",
    "regen": "../../node_modules/.bin/rollup --config 7-regen/rollup.config.js",
    "start": "./node_modules/.bin/cmdline-scripts Start",
    "build": "../../node_modules/.bin/rollup --config",
    "cfg": "../../node_modules/.bin/rollup --config ../../rollup.config.js",
    "clean": "./node_modules/.bin/cmdline-scripts Clean",
    "test": "echo \"170920 rollupapp test nimp\""
  },
  "rollup": {
    "node": true,
    "input": [
      {
        "input": "7-regen/regen",
        "output": {
          "file":"7-regen/regen-cfg",
          "format": "cjs"
        }
      },
      {
        "input": "6-babelhelpers/helper",
        "output": {
          "file":"6-babelhelpers/helper-cfg",
          "format": "cjs"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "keywords": [
    "app"
  ],
  "private": true,
  "dependencies": {
    "spawn-async": "0.0.1"
  }
}
