{
  "name": "porter",
  "version": "0.0.1",
  "description": "Service manager and monitoring",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "../../node_modules/.bin/babel-node src",
    "build": "../../node_modules/.bin/rollup --config node:rollupconfig",
    "clean": "./node_modules/.bin/clean build",
    "test": "../../node_modules/.bin/jest"
  },
  "rollup": {
    "print": true,
    "node": true,
    "nodelatest": false,
    "input": "src/index",
    "output": {
      "file": "build/porter",
      "format": "cjs"
    },
    "shebang": true,
    "clean": "build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haraldrudell"
  },
  "keywords": [
    "Service",
    "route",
    "ip",
    "ping"
  ],
  "private": true,
  "dependencies": {
    "classrunner": "0.0.1",
    "fs-extra": "^4.0.1",
    "getopt2018": "0.0.1",
    "syslogtcp": "0.0.1"
  },
  "devDependencies": {
    "rollupconfig": "0.0.1"
  }
}
