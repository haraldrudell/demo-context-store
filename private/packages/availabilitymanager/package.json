{
  "name": "availabilitymanager",
  "version": "0.0.1",
  "description": "Node.js based service manager",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "ISC",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "./node_modules/.bin/babel-node src/run",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/babel-node config/Build",
    "clean": "./node_modules/.bin/babel-node config/Clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haraldrudell"
  },
  "keywords": [
    "Android",
    "adb",
    "apk",
    "app"
  ],
  "singleFile": {
    "name": "availer",
    "entry": "run"
  },
  "babel": {
    "presets": [
      "node8"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    }
  },
  "private": true,
  "dependencies": {
    "asyncprocess": "0.0.1",
    "classrunner": "0.0.1",
    "clean": "0.0.1",
    "commander": "^2.11.0",
    "cross-env": "^5.0.4",
    "fs-extra": "^4.0.1",
    "js-yaml": "^3.9.1",
    "network": "^0.3.2",
    "rollupconfig": "0.0.1",
    "syslog-client": "^1.1.1",
    "uuid": "^3.1.0"
  }
}
