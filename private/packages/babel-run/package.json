{
  "name": "babel-run",
  "version": "0.0.1",
  "description": "Run commands from parent node_module/.bin directories",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=4.8.1"
  },
  "scripts": {
    "build": "rollup --config",
    "clean": "../../node_modules/.bin/babel-node src/cleanbin",
    "lint": "eslint rollup.config.js",
    "test": "npm-run jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "rollup": {
    "print": true,
    "input": [
      {
        "input": "src/rollup.config",
        "module": true,
        "dependencies": true
      },
      {
        "input": "src/cleanbin.js",
        "output": {
          "file": "build/clean",
          "format": "cjs"
        },
        "shebang": true
      }
    ],
    "clean": [
      "lib",
      "build"
    ],
    "node": true
  },
  "bin": {
    "babel-run": "build/babel-run"
  },
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "keywords": [
    "app"
  ],
  "private": true,
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "rollup": "^0.56.0"
  }
}
