{
  "name": "rollupconfig",
  "version": "0.0.1",
  "description": "Provide base rollup.config.js",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8"
  },
  "module": "lib/rollup.config.js",
  "scripts": {
    "build": "../../node_modules/.bin/rollup --config",
    "clean": "../../node_modules/.bin/babel-node src/cleanbin",
    "test": "../../node_modules/.bin/jest"
  },
  "rollup": {
    "print": true,
    "input": [
      {
        "input": "src/rollup.config",
        "module": true
      },
      {
        "input": "src/cleanbin.js",
        "output": {
          "file": "build/clean",
          "format": "cjs"
        },
        "shebang": true
      }
    ],
    "clean": [
      "lib",
      "build"
    ],
    "dependencies": true,
    "node": true
  },
  "bin": {
    "clean": "build/clean"
  },
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "keywords": [
    "app"
  ],
  "private": true,
  "dependencies": {
    "fs-extra": "",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-shebang": "^0.1.4"
  }
}
