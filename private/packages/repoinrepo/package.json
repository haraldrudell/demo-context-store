{
  "name": "repoinrepo",
  "version": "0.0.1",
  "description": "Manage Forked Repositories inside main Repository by Harald Rudell",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "main": "bin/repoinrepo",
  "scripts": {
    "build": "rollup --config node:es2049package",
    "runbuild": "node bin/starter -d -yamlfile default.yaml",
    "dojest": "es2049scripts -active jest .",
    "watch": "rollup --watch --config node:es2049package",
    "reinstall": "npm-run-all --serial reinstall:*",
    "reinstall:clean": "clean node_modules",
    "reinstall:install": "yarn",
    "clean": "clean bin",
    "tw": "jest --watch",
    "test": "jest"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "rollup": {
    "dependencies": false,
    "targets": "mini",
    "shebang": true
  },
  "dependencies": {
    "allspawn": "^1.0.1",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "es2049package": "^1.0.1",
    "jest": "^23.6.0",
    "test": "0.0.1"
  }
}
