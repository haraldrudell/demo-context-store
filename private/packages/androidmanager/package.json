{
  "name": "androidmanager",
  "version": "0.2.0",
  "description": "Node.js based Android device management",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "ISC",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "babel-node src",
    "build": "rollup --config node:es2049package",
    "build:debug": "node --inspect-brk rollup --config node:es2049package",
    "clean": "clean bin",
    "sockets": "babel-node src/SocketTester.js",
    "test": "../../node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haraldrudell/androidmanager"
  },
  "keywords": [
    "Android",
    "adb",
    "apk",
    "app"
  ],
  "rollup": {
    "targets": "mini",
    "shebang": true,
    "dependencies": true
  },
  "private": true,
  "dependencies": {
    "adbkit": "^2.11.0",
    "fs-extra": "^4.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "es2049package": "^0.0.11"
  }
}
