{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "build/apkmanager",
      "program": "${workspaceRoot}/build/apkmanager.js",
      "stopOnEntry": true,
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "src/run",
      "program": "${workspaceRoot}/src/run.js",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node"
    }
  ]
}