{
  "name": "udptcp",
  "version": "0.0.1",
  "description": "Forward udp inside tcp by Harald Rudell",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "main": "bin/udptcp",
  "scripts": {
    "build": "rollup --config node:es2049package",
    "watch": "rollup --watch --config node:es2049package",
    "tw": "jest --watch",
    "clean": "clean bin",
    "debugtest": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test": "jest"
  },
  "keywords": [
    "email"
  ],
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "rollup": {
    "dependencies": false,
    "targets": "mini",
    "external": ["grpc", "protobufjs"],
    "shebang": true
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  },
  "dependencies": {
    "es2049lib": "0.0.1",
    "es2049options": "0.0.1",
    "grpc": "^1.10.1",
    "protobufjs": "^6.8.8",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "es2049package": "^1.0.1",
    "jest": "^23.6.0"
  }
}
