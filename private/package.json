{
  "name": "sw",
  "version": "1.0.0",
  "description": "A Lerna monolithic repository",
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "scripts":{
    "copyrollup": "cp -a packages/rollupconfig/lib/rollup.config.js ."
  },
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "dependencies": {
    "lerna": "^2.1.0"
  },
  "babel": {
    "babelrc": false,
    "sourceMaps": true,
    "env": {
      "env": {
        "presets": [
          [
            "env"
          ]
        ],
        "plugins": [
          "transform-runtime"
        ]
      },
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "7"
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-generator-functions",
          "transform-runtime"
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "7"
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-generator-functions",
          "transform-runtime"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-node8": "^1.2.0",
    "babel-runtime": "^6.26.0",
    "del": "^3.0.0",
    "eslint": "^4.7.0",
    "eslint-config-fbjs": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-relay": "0.0.8",
    "jest": "^21.1.0",
    "regenerator-runtime": "^0.11.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-shebang": "^0.1.4",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-regenerator-runtime": "^6.23.1",
    "webpack": "^3.6.0",
    "yarn": "^1.0.2"
  }
}
