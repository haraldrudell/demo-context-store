{
  "name": "sw",
  "version": "0.0.1",
  "description": "A private monolithic repository",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clearNodeModules": "for P in packages/*; do NM=\"$P/node_modules\" && if [ -d \"$NM\" ]; then echo \"$NM\"; rm -r \"$NM\"; fi; done"
  },
  "babel": {
    "babelrc": false,
    "sourceMaps": true,
    "env": {
      "env": {
        "presets": [
          [
            "env"
          ]
        ],
        "plugins": [
          "transform-runtime"
        ]
      },
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "7"
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-generator-functions",
          "transform-runtime"
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "7"
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-generator-functions",
          "transform-runtime"
        ]
      }
    }
  }
}
