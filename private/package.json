{
  "name": "sw",
  "version": "0.0.1",
  "description": "A private monolithic repository",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "gt:sw"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clearNodeModules": "for P in packages/*; do NM=\"$P/node_modules\" && if [ -d \"$NM\" ]; then echo \"$NM\"; rm -r \"$NM\"; fi; done"
  },
  "babel": {
    "babelrc": false,
    "sourceMaps": true,
    "env": {
      "env": {
        "presets": [
          [
            "env"
          ]
        ],
        "plugins": [
          "transform-runtime"
        ]
      },
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "7"
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-generator-functions",
          "transform-runtime"
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "7"
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-async-generator-functions",
          "transform-runtime"
        ]
      }
    }
  },
  "resolutions": {
    "fsevents": "^1.2.4",
    "grpc": "^1.16.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^4.19.1",
    "eslint-config-fbjs": "^2.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-relay": "~0.0.8",
    "rollup-plugin-eslint": "^5.0.0"
  }
}
