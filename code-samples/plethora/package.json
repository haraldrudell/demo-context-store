{
  "name": "plethora",
  "version": "0.0.1",
  "description": "Service manager and monitoring",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "nmrun babel-node src/run",
    "dev": "micro-dev",
    "build": "rollup --config node:rollupconfig",
    "test": "jest"
  },
  "keywords": [
    "Service",
    "route",
    "ip",
    "ping"
  ],
  "private": true,
  "babel": {
    "babelrc": false,
    "sourceMaps": true,
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-function-bind"
    ]
  },
  "dependencies": {
    "fs-extra": "^4.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "micro-dev": "^1.3.0",
    "node-fetch": "^1.7.3",
    "test-listen": "^1.0.2"
  }
}
