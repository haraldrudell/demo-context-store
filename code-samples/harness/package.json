{
  "name": "harness",
  "version": "1.0.0",
  "description": "harness.io",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config config5/webpack.config.js",
    "config": "eslint config/webpack.config.js config/rollup.config.js config/styleguide.config.js && rollup --config config/rollup.config.js",
    "styleguide": "styleguidist server --config config5/styleguide.config.js",
    "styleguide:build": "styleguidist build --config config5/styleguide.config.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": "gt:sw",
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-undef": 0,
      "no-restricted-globals": 0
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^1.34.1",
    "@blueprintjs/datetime": "^1.25.0",
    "@blueprintjs/table": "^1.31.0",
    "@harnessio/mentions": "^1.1.51",
    "ag-grid": "^14.2.0",
    "ag-grid-react": "^14.2.0",
    "ansi-to-html": "^0.6.3",
    "atmosphere.js": "^2.3.3",
    "expandable-view": "^1.0.10",
    "fetch-plus": "^3.10.6",
    "fetch-plus-basicauth": "^3.10.4",
    "fetch-plus-bearerauth": "^3.10.4",
    "fetch-plus-json": "^3.10.4",
    "gregorian-calendar": "^4.1.6",
    "history": "^4.7.2",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "optional-tooltip": "^1.2.8",
    "pick-react-known-prop": "^0.1.2",
    "plain-cache": "^1.1.12",
    "plotly.js": "^1.31.2",
    "pubsub-js": "^1.5.7",
    "rc-calendar": "^9.3.1",
    "rc-tree": "^1.7.10",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-update": "^15.6.2",
    "react-async-script-loader": "^0.3.0",
