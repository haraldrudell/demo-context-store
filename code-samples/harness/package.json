    "react-infinite": "^0.12.1",
    "react-intl": "^2.4.0",
    "react-joyride": "^1.11.1",
    "react-jsonschema-form": "^1.0.0",
    "react-monaco-editor": "^0.12.0",
    "react-motion": "^0.5.2",
    "react-notification-system": "^0.2.16",
    "react-plotlyjs": "^0.4.4",
    "react-router": "^3.2.0",
    "react-router-transition": "^1.1.2",
    "react-select": "^1.1.0",
    "react-setup-transmit": "^3.1.8",
    "react-sortable-tree": "^1.5.2",
    "react-tagsinput": "^3.19.0",
    "react-timeago": "^3.4.3",
