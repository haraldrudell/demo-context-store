  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node --experimental-modules --no-warnings src/index",
    "exports": "node --experimental-modules --no-warnings src/exports",
    "dobabel": "es2049scripts -active babel .",
    "test": "jest"
  },
  "keywords": [
    "email"
  ],
  "repository": {
    "type": "git",
