{
  "name": "demo-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel 'start:*'",
    "start:react": "BROWSER=none react-scripts start",
    "start:e": "npm-run-all --serial 'ele:*'",
    "ele:w": "sleep 2",
    "ele:go": "electron src/electron",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.11",
    "npm-run-all": "^4.1.5"
  }
}
