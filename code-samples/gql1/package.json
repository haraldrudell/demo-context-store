{
  "name": "gql1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "color": "^3.1.0",
    "react": "^16.6",
    "react-dom": "^16.6",
    "react-jss": "^8.6.1",
    "react-scripts": "2.0.5",
    "styled-components": "^4.0.2",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "start": "npm-run-all --serial linkfonts start:react",
    "start:react": "react-scripts start",
    "build": "npm-run-all --serial unlinkfonts build:react",
    "build:react": "react-scripts build",
    "test": "npm-run-all --serial linkfonts test:react",
    "test:react": "react-scripts test",
    "linkfonts": "node --experimental-modules --no-warnings scripts/linkfonts.mjs yes",
    "unlinkfonts": "node --experimental-modules --no-warnings scripts/linkfonts.mjs no",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "fs.extra": "^1.3.2",
    "npm-run-all": "^4.1.3",
    "resolve": "^1.8.1"
  }
}
