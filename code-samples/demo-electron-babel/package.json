{
  "name": "demo-electron-babel",
  "version": "0.0.1",
  "description": "Demonstrate stand-alone Electron using babel 7 by Harald Rudell",
  "license": "UNLICENSED",
  "scripts": {
    "start": "electron src",
    "start0": "npm-run-all --serial 'start0:*'",
    "start0:babel": "es2049scripts -active babel .",
    "start0:el": "electron --require @babel/register src/original",
    "start00": "electron src/original0",
    "dobabel": "es2049scripts -active babel ."
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "electron": "^3.0.7",
    "es2049scripts": "^1.0.0",
    "npm-run-all": "^4.1.5"
  }
}
