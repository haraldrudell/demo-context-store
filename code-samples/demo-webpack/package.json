        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@purtuga/esm-webpack-plugin": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2"
  }
