  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "UNLICENSED",
  "main": "lib/cjs",
  "module": "lib/esm",
  "private": true,
  "scripts": {
    "build": "webpack",
    "reinstall": "npm-run-all --serial reinstall:*",
    "reinstall:clean": "rimraf node_modules",
    "reinstall:install": "yarn",
    "testBabel": "node src/testBabelRegister",
