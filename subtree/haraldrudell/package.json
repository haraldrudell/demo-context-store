{
  "name": "haraldrudell",
  "version": "0.0.1",
  "description": "https://haraldrudell/github.io/haraldrudell",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "ISC",
  "keywords": [
    "Harald Rudell",
    "Online Apps",
    "Portfolio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haraldrudell/haraldrudell.git"
  },
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm-run-all --serial 'deploy:*'",
    "deploy:test": "CI=1 react-scripts test --all",
    "deploy:build": "yarn build",
    "deploy:readme": "cp src/README.md build",
    "deploy:reset": "rimraf /tmp/haraldrudell",
    "deploy:clone": "git clone --branch=gh-pages --single-branch --depth=1 https://github.com/haraldrudell/haraldrudell.git /tmp/haraldrudell",
    "deploy:remove": "rm -r /tmp/haraldrudell/*",
    "deploy:copy": "cp -a build/. /tmp/haraldrudell",
    "deploy:add": "git --git-dir=/tmp/haraldrudell/.git --work-tree=/tmp/haraldrudell add /tmp/haraldrudell",
    "deploy:commit": "git --git-dir=/tmp/haraldrudell/.git commit --message=Update",
    "deploy:push": "git --git-dir=/tmp/haraldrudell/.git push origin gh-pages",
    "test": "react-scripts test"
  },
  "homepage": "https://haraldrudell.github.io/haraldrudell",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-scripts-port0": "^2.1.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2"
  }
}
