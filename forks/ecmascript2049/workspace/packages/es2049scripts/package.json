{
  "name": "es2049scripts",
  "version": "0.0.9",
  "description": "ECMAScript 2049: ES.Next on every line by triple-transpile by Harald Rudell: ERASE THE PAST",
  "author": "Harald Rudell <harald.rudell@gmail.com> (http://haraldrudell.com)",
  "license": "ISC",
  "bin": {
    "es2049scripts": "bin/es2049scripts"
  },
  "scripts": {
    "build": "node config",
    "transpilezero": "npx npm-run-all transpilezero:eslint transpilezero:babel transpilezero:transpile",
    "transpilezero:eslint": "eslint --ext .mjs transpilezero",
    "transpilezero:babel": "babel --out-file config/transpilezero.js --extensions .mjs transpilezero/transpile-zero.mjs transpilezero/ZeroTranspiler.mjs",
    "transpilezero:transpile": "node config/transpilezero",
    "testmjs": "node --experimental-modules src/esmodule.test.mjs",
    "testProject": "node config/scripts-test",
    "test": "jest"
  },
  "babel": {
    "env": {
      "development": {
        "babelrc": false,
        "sourceMaps": true,
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "6"
              }
            }
          ],
          "stage-0"
        ],
        "plugins": [
          "external-helpers",
          "transform-runtime"
        ]
      },
      "test": {
        "babelrc": false,
        "sourceMaps": true,
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          "stage-0"
        ],
        "plugins": [
          "external-helpers",
          "transform-runtime"
        ]
      },
      "rollup": {
        "babelrc": false,
        "sourceMaps": true,
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "4.8.1"
              },
              "modules": false
            }
          ],
          "stage-0"
        ],
        "plugins": [
          "external-helpers",
          "transform-runtime"
        ]
      }
    }
  },
  "keywords": [
    "ECMAScript",
    "ECMAScript 2049",
    "ES.Next",
    "JavaScript",
    "Transpile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haraldrudell/ECMAScript2049"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.14.0",
    "eslint-plugin-babel": "^4.1.2",
    "fs-extra": "^5.0.0",
    "resolve": "^1.5.0",
    "rollup": "^0.53.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-shebang": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "jest": "^22.0.4",
    "npm-run-all": "^4.1.2"
  }
}
