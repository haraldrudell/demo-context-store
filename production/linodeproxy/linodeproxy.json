{
	"api": {
		"apiMap": {
			"linodeproxy": {
				"onLoad": true,
				"port": 1080,
				"httpProxyOptions": {
					"hostnameOnly": true,
					"router": {
						"localhost": "127.0.0.1:80",
						"*": {
							"statusCode": 404,
							"headers": {
								"Content": "text/html"
							},
							"data": "<!doctype html><meta charset=\"UTF-8\"/><title>Hostname undefined</title><body><h1>Hostname undefined</h1></body>"
						}
					}
				}
			}
		}
	}
}
